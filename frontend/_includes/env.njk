<script>
  // Initialize environment variables
  window.ENV = {
    API_BASE_URL: (function() {
      var url = '{{ env.API_BASE_URL | default: "" }}';
      if (!url) {
      console.warn('env.API_BASE_URL is undefined, using default "http://localhost:8000"');
      return "http://localhost:8000";
      }
      return url;
    })()
  };
</script>
