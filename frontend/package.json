{
  "name": "lynriescope-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npx tsc --watch\" \"npx @11ty/eleventy --serve\"",
    "build": "tsc && npx @11ty/eleventy",
    "lint": "eslint \"**/*.{js,ts}\" --ignore-pattern \"_site/**\" --max-warnings=0",
    "lint:fix": "eslint \"**/*.{js,ts}\" --ignore-pattern \"_site/**\" --fix",
    "format": "prettier --write \"**/*.{js,ts,json,css,html}\" --ignore-path .prettierignore",
    "format:check": "prettier --check \"**/*.{js,ts,json,css,html}\" --ignore-path .prettierignore",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kenrie19/LynrieScoop.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kenrie19/LynrieScoop/issues"
  },
  "homepage": "https://github.com/Kenrie19/LynrieScoop#readme",
  "description": "",
  "dependencies": {
    "@11ty/eleventy": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/css": "^0.8.0",
    "@eslint/js": "^9.26.0",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.4.0",
    "@types/node": "^22.15.29",
    "better-docs": "^2.7.3",
    "clean-jsdoc-theme": "^4.3.0",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "jsdoc-plugin-typescript": "^3.2.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  }
}
